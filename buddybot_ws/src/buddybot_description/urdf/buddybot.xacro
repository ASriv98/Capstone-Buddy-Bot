<?xml version="1.0"?>
<!-- Revolute-Revolute Manipulator -->
<robot name="buddybot" xmlns:xacro="http://www.ros.org/wiki/xacro">


  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find buddybot_description)/urdf/buddybot.gazebo" />

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  
  <material name="black">
    <color rgba="0 0 0 0"/>
  </material>

  <material name="cyan">
    <color rgba="0 0.9 0.9 1.0"/>
  </material>


  <link name="base_link"/>

  <link name="body">
    <visual>
      <geometry>
        <box size=".6096 .4572 .3072"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="cyan"/>
    </visual>
    <collision>
      <geometry>
        <box size=".6096 .4572 .3072"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.38832" ixy="0.0" ixz="0.0" iyy="0.48387" iyz="0.0" izz="0.25283"/>
    </inertial>
  </link>

  <joint name = 'base_link_to_body' type = 'fixed'>
    <parent link = 'base_link'/>
    <child link = 'body'/>
    <origin rpy="0 0 0" xyz="0 0 .5"/>
  </joint>


  <link name = "front_wheel_bar">
    <visual>
      <geometry>
        <box size = ".1 .67 .14"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name = "blue"/>
    </visual>
    <collision>
      <geometry>
        <box size = ".1 .67 .14"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.002466" ixy="0.0" ixz="0.0" iyy="0.038241" iyz="0.0" izz="0.039041"/>
    </inertial>
  </link>

  <joint name = 'base_to_front_wheel_bar' type = 'fixed'>
    <parent link = 'body'/>
    <child link = 'front_wheel_bar'/>
    <origin xyz = '.28 0 -.2'/>
  </joint>

<!-- LEFT WHEEL -->
  <link name="left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.095"/>
      </geometry>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.095"/>
      </geometry>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="3"/>
      <inertia ixx="0.0092687" ixy="0.0" ixz="0.0" iyy="0.0092687" iyz="0.0" izz="0.0022563"/>
    </inertial>
  </link>

  <joint name="left_wheel_joint" type="continuous">
    <parent link="front_wheel_bar"/>
    <child link="left_wheel"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0 0.34 -.08"/>
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="left_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="left_wheel_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor_left">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

<!-- RIGHT WHEEL -->
  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.095"/>
      </geometry>
      <origin rpy="-1.6 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.095"/>
      </geometry>
      <origin rpy="-1.6 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="3"/>
      <inertia ixx="0.0092687" ixy="0.0" ixz="0.0" iyy="0.0092687" iyz="0.0" izz="0.0022563"/>
    </inertial>
  </link>

  <joint name="right_wheel_joint" type="continuous">
    <parent link="front_wheel_bar"/>
    <child link="right_wheel"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0 -0.34 -.08"/>
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="right_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="right_wheel_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor_right">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

<!-- BACK WHEEL -->
  <link name = "back_wheel_bar">
    <visual>
      <geometry>
        <box size = ".1 .1 .14"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name = "blue"/>
    </visual>
    <collision>
      <geometry>
        <box size = ".1 .1 .14"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.002466" ixy="0.0" ixz="0.0" iyy="0.001666" iyz="0.0" izz="0.002466"/>
    </inertial>
  </link>

  <joint name = 'base_to_back_wheel_bar' type = 'fixed'>
    <parent link = 'body'/>
    <child link = 'back_wheel_bar'/>
    <origin xyz = '-.28 0 -.2'/>
  </joint>

  <link name="back_wheel">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.095"/>
      </geometry>
      <origin rpy="-1.6 0 0" xyz="-0.1 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.095"/>
      </geometry>
      <origin rpy="-1.6 0 0" xyz="-0.1 0 0"/>
    </collision>
    <inertial>
      <mass value="3"/>
      <inertia ixx="0.0092687" ixy="0.0" ixz="0.0" iyy="0.0092687" iyz="0.0" izz="0.0022563"/>
    </inertial>
  </link>

  <joint name="back_bar_to_back_wheel" type="continuous">
    <parent link="back_wheel_bar"/>
    <child link="back_wheel"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 -.08"/>
  </joint>


 <!-- Hokuyo Laser -->

  <link name="hokuyo_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	<box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://buddybot_description/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

  <joint name = 'lidar_to_body' type="fixed">
    <parent link="body"/>
    <child link="hokuyo_link"/>
    <origin xyz="0 0 .1886"/>
  </joint>

  <!-- Realsense -->

  <link name="realsense">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.073000 0.276000 0.072000"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://buddybot_description/meshes/kinect.dae"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="9.108e-05" ixy="0.0" ixz="0.0" iyy="2.51e-06" iyz="0.0" izz="8.931e-05"/>
    </inertial>
  </link>

  <link name = 'camera_mount'>
    <visual>
      <geometry>
        <box size = '.03 .03 .42'/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 -.21"/>
      <material name = 'cyan'/>
    </visual>
    <collision>
      <geometry>
        <box size = '.03 .03 .42'/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.01477" ixy="0.0" ixz="0.0" iyy="0.00015" iyz="0.0" izz="0.01477"/>
    </inertial>
  </link>

  <joint name="base_to_camera_mount" type="fixed">
    <parent link="body"/>
    <child link="camera_mount"/>
    <origin xyz="-.3048 0 .50"/>
  </joint>

  <joint name = 'camera_to_mount' type="fixed">
    <parent link="camera_mount"/>
    <child link="realsense"/>
    <origin xyz=".04 0 .032"/>
  </joint>


  <link name = 'front_right_top'>
    <visual>
      <geometry>
        <box size = '.03 .03 .35'/>
      </geometry>
      <origin rpy="0 -.6 0" xyz=".4 0.215 0"/>
      <material name = 'cyan'/>
    </visual>
    <collision>
      <geometry>
        <box size = '.03 .03 .35'/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.01028" ixy="0.0" ixz="0.0" iyy="0.00015" iyz="0.0" izz="0.01028"/>
    </inertial>
  </link>

  <link name = 'front_left_top'>
    <visual>
      <geometry>
        <box size = '.03 .03 .35'/>
      </geometry>
      <origin rpy="0 -.6 0" xyz=".4 -0.215 0"/>
      <material name = 'cyan'/>
    </visual>
    <collision>
      <geometry>
        <box size = '.03 .03 .35'/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.01028" ixy="0.0" ixz="0.0" iyy="0.00015" iyz="0.0" izz="0.01028"/>
    </inertial>
  </link>

  <link name = 'front_right_bottom'>
    <visual>
      <geometry>
        <box size = '.03 .03 .1677'/>
      </geometry>
      <origin rpy="0 -1.6 0" xyz="0.42 .215 -.13"/>
      <material name = 'cyan'/>
    </visual>
    <collision>
      <geometry>
        <box size = '.03 .03 .1677'/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.002419" ixy="0.0" ixz="0.0" iyy="0.00015" iyz="0.0" izz="0.002419"/>
    </inertial>
  </link>

  <link name = 'front_left_bottom'>
    <visual>
      <geometry>
        <box size = '.03 .03 .1677'/>
      </geometry>
      <origin rpy="0 -1.6 0" xyz="0.42 -.215 -.13"/>
      <material name = 'cyan'/>
    </visual>
    <collision>
      <geometry>
        <box size = '.03 .03 .1677'/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.002419" ixy="0.0" ixz="0.0" iyy="0.00015" iyz="0.0" izz="0.002419"/>
    </inertial>
  </link>

  <link name = 'front_connector'>
    <visual>
      <geometry>
        <box size = '.03 .03 .4572'/>
      </geometry>
      <origin rpy="1.6 0 0" xyz=".5 0 -.13"/>
      <material name = 'cyan'/>
    </visual>
    <collision>
      <geometry>
        <box size = '.03 .03 .4572'/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.01749" ixy="0.0" ixz="0.0" iyy="0.00015" iyz="0.0" izz="0.01749"/>
    </inertial>
  </link>

  <joint name = 'F_R_T_to_base' type = 'fixed'>
    <parent link="body"/>
    <child link="front_right_top"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name = 'F_L_T_to_base' type = 'fixed'>
    <parent link="body"/>
    <child link="front_left_top"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name = 'F_R_B_to_base' type = 'fixed'>
    <parent link="body"/>
    <child link="front_right_bottom"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name = 'F_L_B_to_base' type = 'fixed'>
    <parent link="body"/>
    <child link="front_left_bottom"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name = 'front_connector_to_FRT' type = 'fixed'>
    <parent link="front_right_top"/>
    <child link="front_connector"/>
    <origin xyz="0 0 0"/>
  </joint>

</robot>
